% basis set specification
basis<GaussianBasisSet>: (
  name = "6-311G"
  molecule = $:molecule
  puream = true
)

mpqc: (
  checkpoint = no
  restart = no
  savestate = no
  do_energy = yes
  do_gradient = no
  mole<CLHF>: (
    molecule = $:molecule
     basis = $:basis
     memory = 2GB
     value_accuracy = 1e-7
     guess_wavefunction<CLHF>: (
       lindep_tol = 1e-10
       molecule = $:molecule
       value_accuracy = 1e-6
       memory = 2GB
       basis<GaussianBasisSet>: (
         molecule = $:molecule
         name = "STO-3G"
       )
     )
   )
)
